<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="tile_frame">
    <property name="cursor">none</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Canvas" id="image_canvas">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nsew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="timestamp_label">
        <property name="anchor">center</property>
        <property name="font">TkDefaultFont</property>
        <property name="text" translatable="yes">1234</property>
        <property name="textvariable">string:timestamp_text</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">sew</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
